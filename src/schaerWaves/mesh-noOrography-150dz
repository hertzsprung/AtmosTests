FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

convertToMeters 1000;

vertices (
    (-150.125 0 0)  // 0
    (150.125  0 0)  // 1
    (150.125  1 0)  // 2
    (-150.125 1 0)  // 3
    (-150.125 0 30) // 4
    (150.125  0 30) // 5
    (150.125  1 30) // 6
    (-150.125 1 30) // 7
);

blocks (
	hex (0 1 2 3 4 5 6 7)
	    (1201 1 200)
	    simpleGrading (1 1 1)
);

boundary (
	inlet {
		type inlet;
		faces (
			(0 3 7 4)
		);
	}

	outlet {
		type outlet;
		faces (
			(1 2 6 5)
		);
	}

	ground {
		type wall;
		faces (
			(0 1 2 3)
		);	
	}

	top {
		type wall;
		faces (
			(4 5 6 7)
		);
	}

	frontAndBack {
		type empty;
		faces (
			(0 1 5 4)
			(3 2 6 7)
		);
	}
);
