FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeMeshDict;
}

constructFrom patch;

sourceCase "$SOURCE_CASE";
sourcePatches (otherSide);

exposedPatchName "originalPatch";

