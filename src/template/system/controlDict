FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

startFrom       startTime;
startTime       0;
stopAt          endTime;
endTime         $END_TIME;
deltaT          $TIMESTEP;
writeControl    runTime;
writeInterval   $WRITE_INTERVAL;
purgeWrite      0;
writeFormat     binary;
writePrecision  6;
writeCompression uncompressed;
timeFormat      general;
timePrecision   6;
runTimeModifiable yes;
adjustTimeStep no;
maxCo          1;
libs           ("libOpenFOAM.so" "libExnerTheta.so" "libspecie.so" "libfiniteVolumeUser.so");

functions {
    courantNo {
        type          CourantNo;
        functionObjectLibs ("libutilityFunctionObjects.so");

        outputControl outputTime;
        timeInterval  1;
    }
}
