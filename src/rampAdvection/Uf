FoamFile
{
    version     2.2;
    format      ascii;
    class       surfaceVectorField;
    location    "constant";
    object      Uf;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 1);

boundaryField
{
    ground
    {
        type            fixedValue;
        value           uniform (1 0 1);
    }
    top
    {
        type            fixedValue;
        value           uniform (1 0 1);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 1);
    }
    outlet
    {
        type            fixedValue;
        value           uniform (1 0 1);
    }
}
